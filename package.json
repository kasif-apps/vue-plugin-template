{
  "name": "vue-plugin-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Kasif plugin template with Vue",
  "scripts": {
    "dev": "vite",
    "build": "node ./.kasif/build.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@kasif-apps/app": "^0.0.0",
    "@kasif-apps/plugin-compiler": "^0.0.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  },
  "kasif": {
    "name": "Vue Plugin Template",
    "identifier": "vue-plugin-template",
    "lib": {
      "dir": "lib",
      "entry": "index"
    },
    "backend": {
      "dir": "remote",
      "entry": "index"
    },
    "permissions": [
      "push_view",
      "show_notifications"
    ]
  }
}
